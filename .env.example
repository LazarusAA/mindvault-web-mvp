# Required Environment Variables
NODE_ENV=development
NEXT_PUBLIC_API_URL=http://localhost:3000
NEXT_PUBLIC_ENVIRONMENT=development
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-32-char-secret-here

# Database (Required)
NEXT_PUBLIC_XATA_API_KEY=your-xata-key
NEXT_PUBLIC_XATA_BRANCH=main

# Web3 (Required for wallet features)
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your-project-id

# Security (Required)
JWT_SECRET=your-jwt-secret-min-32-chars
ENCRYPTION_KEY=your-encryption-key-32-chars

# Optional Features
NEXT_PUBLIC_ENABLE_WORLDID=false
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_MAINTENANCE_MODE=false

# Development Tools
ANALYZE=false
NEXT_TELEMETRY_DISABLED=1

# Rate Limiting (Optional, defaults shown)
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000

# Cache Control (Optional, defaults shown)
CACHE_MAX_AGE=31536000
STALE_WHILE_REVALIDATE=86400

# Note: For production, additional variables are required. See .env.production

# Turbo
TURBO_TEAM=your-team-name
TURBO_TOKEN=your-token 